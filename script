local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/jensonhirst/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "BALL3RHUB", HidePremium = false, SaveConfig = true, ConfigFolder = "ConfigFolder"})

local Tab = Window:MakeTab({ Name = "MURDERMYSTERY2", Icon = "rbxassetid://4483345998", PremiumOnly = false })

local Section = Tab:AddSection({ Name = "MurderMystery2 scripts" })


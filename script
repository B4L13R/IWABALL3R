local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/jensonhirst/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "BALL3RHUB", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

--[[
Name = <string> - The name of the UI.
HidePremium = <bool> - Whether or not the user details shows Premium status or not.
SaveConfig = <bool> - Toggles the config saving in the UI.
ConfigFolder = <string> - The name of the folder where the configs are saved.
IntroEnabled = <bool> - Whether or not to show the intro animation.
IntroText = <string> - Text to show in the intro animation.
IntroIcon = <string> - URL to the image you want to use in the intro animation.
Icon = <string> - URL to the image you want displayed on the window.
CloseCallback = <function> - Function to execute when the window is closed.
]]

local Tab = Window:MakeTab({
	Name = "SCRIPTS",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

--[[
Name = <string> - The name of the tab.
Icon = <string> - The icon of the tab.
PremiumOnly = <bool> - Makes the tab accessible to Sirus Premium users only.
]]

local Section = Tab:AddSection({
	Name = "MurderMystery2 scripts"
})

--[[
Name = <string> - The name of the section.
]]

OrionLib:MakeNotification({
	Name = "Welcome:)",
	Content = "Welcome to BALL3RHUB",
	Image = "rbxassetid://4483345998",
	Time = 5
})

--[[
Title = <string> - The title of the notification.
Content = <string> - The content of the notification.
Image = <string> - The icon of the notification.
Time = <number> - The duration of the notfication.
]]

Tab:AddButton({
	Name = "HIGHLIGHTHUB_key hlvipcomingsoon(its a upper case i)",
	Callback = function()
      	loadstring(game:HttpGet("https://raw.githubusercontent.com/ThatSick/HighlightMM2/main/Main"))()
  	end    
})

--[[
Name = <string> - The name of the button.
Callback = <function> - The function of the button.
]]

Tab:AddButton({
	Name = "VynixuMM2",
	Callback = function()
      loadstring(game:HttpGet("https://raw.githubusercontent.com/Neon-Fox/roblox-scripts/main/VynixuMM2"))()
  	end    
})

--[[
Name = <string> - The name of the button.
Callback = <function> - The function of the button.
]]

Tab:AddButton({
	Name = "Xenny-Ware doesnt really work well",
	Callback = function()
      repeat wait() until game.Players.LocalPlayer.Character

url = "https://raw.githubusercontent.com/xennyy/Xenny-Ware/main/loader.lua"

loadstring(game:HttpGet(url))()
  	end    
})

Tab:AddButton({
	Name = "ECLIPSE",
	Callback = function()
      loadstring(game:HttpGet(('https://raw.githubusercontent.com/Lookatit7626/Project-R/main/ECLIPSEHUB.lua')))()
  	end    
})

Tab:AddButton({
	Name = "BALL3RHUBTROLL",
	Callback = function()
     loadstring(game:HttpGet("https://raw.githubusercontent.com/B4L13R/BALL3RMM2/refs/heads/main/script"))()
  	end    
})

Tab:AddButton({
	Name = "TORALSme",
	Callback = function()
      loadstring(game:HttpGet("https://raw.githubusercontent.com/ToraIsMe/ToraIsMe/main/0mm2", true))()
  	end    
})

OrionLib:Init()
